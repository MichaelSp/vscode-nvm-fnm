{
  "name": "vscode-nvm-fnm",
  "displayName": "vscode nvm/fnm switcher",
  "description": "nodejs version auto switcher based using nvm/fnm",
  "version": "0.0.3",
  "license": "MIT",
  "author": {
    "name": "nojsja"
  },
  "repository": {
    "url": "https://github.com/MichaelSp/vscode-nvm-fnm.git"
  },
  "publisher": "nojsja",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.nvmrc"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "package": "yarn vsce package",
    "publish": "yarn vsce publish",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vsce": "^1.53.2",
    "vscode": "^1.1.37"
  }
}